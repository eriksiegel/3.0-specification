default namespace p = "http://www.w3.org/ns/xproc"
namespace local = ""

# This schema could be made more constrained.

Step = Step-xquery

Step-xquery =
  element xquery {
    name.ncname.attr?,
    common.attributes,
    use-when.attr?,
    step.attributes,
    attribute parameters { xsd:string }?,
    attribute version { xsd:string? }?,
    (WithInput* & WithOption* & (Documentation|PipeInfo)*)
  }

